{
  "name": "goes",
  "title": "go+es",
  "version": "1.0.0",
  "description": "go+es",
  "goes": "src/index.html",
  "main": "index.js",
  "targets": {
    "goes": {
      "distDir": "dist"
    }
  },
  "scripts": {
    "build": "parcel build src/index.html",
    "pretty:fix": "prettier --write .",
    "pretty": "prettier --check .",
    "start": "parcel src/index.html --port 3001 --hmr-port 3001",
    "test": "jest",
    "cy:open:qa": "cypress open --env configFile=qa",
    "cy:run:qa": "cypress run --env configFile=qa",
    "cy:open:prod": "cypress open --env configFile=production",
    "cy:run:prod": "cypress run --env configFile=production",
    "cy:open:dev": "cypress open --env configFile=development",
    "cy:run:dev": "cypress run --env configFile=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxfordni/cloud-sample-app.git"
  },
  "keywords": [
    "go",
    "elasticsearch"
  ],
  "author": {
    "name": "ONI",
    "url": "https://oni.bio/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oxfordni/cloud-sample-app/issues"
  },
  "homepage": "https://github.com/oxfordni/cloud-sample-app#readme",
  "dependencies": {
    "antd": "^4.18.5",
    "parcel": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@parcel/transformer-sass": "2.2.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "babel-jest": "^27.4.6",
    "cypress": "^9.4.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^2.3.0",
    "dotenv": "^16.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "tsify": "^5.0.4",
    "typescript": "^4.5.5"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step_definitions/"
  },
  "typeRoots": [
    "./node_modules/@types",
    "./types"
